{
  "author": "Titux Metal <github@lgdweb.fr>",
  "description": "A template for start coding React projects faster.",
  "license": "MIT",
  "main": "index.js",
  "name": "react-projetc-template",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "clear": "rm -rf ./.cache ./dist",
    "prebuild": "yarn clear",
    "build": "parcel build ./src/index.html --public-url ./",
    "lint": "prettier --write 'src/**/*.js' src/index.html",
    "watch": "parcel src/index.html -p 3000"
  },
  "browserslist": [
    ">10%",
    "chrome 76",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-kentcdodds": "^14.14.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TituxMetal/react-project-template"
  }
}
