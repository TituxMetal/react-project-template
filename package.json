    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.4.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.3.0",
    "eslint-config-kentcdodds": "^19.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.17",
